<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
     
      <getPlaceholderRenderings>
        <processor patch:instead="processor[@type='Sitecore.Pipelines.GetPlaceholderRenderings.GetAllowedRenderings, Sitecore.Kernel']"
                   type="CodeTest.Foundation.DynamicPlaceholder.Pipelines.Renderings.GetDynamicKeyAllowedRenderings, CodeTest.Foundation.DynamicPlaceholder" />
      </getPlaceholderRenderings>
    
    </pipelines>
    <settings>
      <setting name="DynamicPlaceholderPattern" value="_dph" />
    </settings>
  </sitecore>
</configuration>